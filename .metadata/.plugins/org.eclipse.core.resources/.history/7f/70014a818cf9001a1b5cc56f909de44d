
public class Test {
	public static void main(String[] args) {
		//インスタンス化
		String str  = "あああ";
		String str2 = "あああ";
		System.out.println(str == str2);
	    String str3 = new String("あああ");
		String str4 = "あああ";
		System.out.println(str3 == str4);
	    String str5 = new String("あああ");
		String str6 = new String("あああ");
		System.out.println(str5.equals(str6));
	}
	
	
}
